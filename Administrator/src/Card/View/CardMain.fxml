<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="300.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Card.Control.CardMainController">
   <left>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children> 
                <Button fx:id="BtnWritePane" disable="true" mnemonicParsing="false" onAction="#BtnWritePane" prefWidth="100.0" text="Card Admin">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Button>
                <Button fx:id="BtnRollPane" mnemonicParsing="false" onAction="#BtnRollPane" prefWidth="100.0" text="Inventory">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Button>
                <Button fx:id="BtnReadPane" mnemonicParsing="false" onAction="#BtnReadPane" prefWidth="100.0" text="Financials">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Button>
         </children>
         <BorderPane.margin>
            <Insets top="20.0" />
         </BorderPane.margin>
         <padding>
            <Insets left="10.0" />
         </padding>
      </VBox>
   </left>
</BorderPane>
